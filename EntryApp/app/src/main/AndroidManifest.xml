    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="EntryApp"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="EntryApp" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UsherColorScreen"
            android:label="@string/title_activity_usher_color_screen" >
        </activity>
        <activity
            android:name=".TicketList"
            android:label="@string/title_activity_ticket_list" >
        </activity>
        <activity
            android:name=".FanSelectedTicket"
            android:label="@string/title_activity_fan_selected_ticket" >
        </activity>
        <activity
            android:name=".FanValidatedTicket"
            android:label="@string/title_activity_validated_ticket" >
        </activity>
        <activity
            android:name=".UsherValidatedTicketList"
            android:label="@string/title_activity_usher_validated_ticket_list" >
        </activity>
    </application>

</manifest>
